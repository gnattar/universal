if (1) % batch from Nov. 2011

  % inj markers
  spacing = 400; % um
  Y = (-1.5:1:1.5)*spacing;
  X = (-1:1:1)*spacing;

  rootd = '/media/misc_data_1/Intrinsic Imaging/';
	anims = {'an155545', 'an155546','an156296','an156299','an156297', 'an156298'};
	tags = {{'c1','c2'},{'c1','c3'},{'c1','c2'},{'c1','c2'},{'c1','c2'}, {'c1','c2'}};
	marks = {[225 165],[250 100],[200 110],[215 100],[225 90],[195 130]};
	moffs = {[200 0],[200 200],[200 0],[200 0],[200 0], [200 0]};
	coords = {[184 180; 210 170],...
						[175 200; 230 170],...
						[180 180; 215 150],...
						[140 115; 160 100],...
						[150 115; 175 100],...
						[170 120; 200 90]};
	am_s_coords = { [264 162 185 191 277 50], ...
									[285 205 262 136 360 128], ...
									[], ...
									[190 80 305 100 284 178], ...
									[180 150 281 114 217 81], ...
									[]}; % isi image affmat employed corsd
	am_t_coords = { [236 257 299 123 318 422], ...
									[225 190 330 261 204 385], ...
									[], ...
									[373 289 191 376 142 243], ...
									[313 156 218 318 330 297], ...
									[], ...
									}; % 4x 2p image affmat coords
	twop_im_path = {'/media/misc_data_1/GECI Expression Project/Main project -- by animal/an155545/an155545_2011_12_01_dz50_4x_001.tif', ...
								'/media/misc_data_1/GECI Expression Project/Main project -- by animal/an155546/an155546_2011_12_01_dz50_4x_001.tif', ...
								'', ... % POOR EXPRESSION -- NO IMAGE
								'/media/misc_data_1/GECI Expression Project/Main project -- by animal/an156299/init_2011_12_02_an156299_dz50_4x_001.tif', ...
								'/media/misc_data_1/GECI Expression Project/Main project -- by animal/an156297/init_2011_12_02_an156297_dz50_4x_001.tif', ...
								'', ... % POOR EXPRESSION
								};
	greenf = {'2011_11_27_green',...
		'2011_11_27_green',...
		'2011_11_27_green',...
		'2011_11_27_green',...
		'2011_11_27_green',...
		'2011_12_05_green'};

	for a=1:length(anims)
	%for a=2
		gfn = [rootd anims{a} filesep greenf{a}];
		cd([rootd anims{a}]);

		if (length(twop_im_path{a} ) > 0)
			im = load_image(twop_im_path{a});
			avim = nanmean(im,3);
			avim = fliplr(avim);
			p_s =am_s_coords{a}; 
			p_t = am_t_coords{a};
			affmat = compute_affmat(p_s, p_t);
			isi_plot(avim'*10, coords{a}(1,:), coords{a}(2,:), tags{a}, marks{a},1,moffs{a},affmat, X, Y);
		else % only plot ISI if you dont have 2p
			isi_plot(gfn, coords{a}(1,:), coords{a}(2,:), tags{a}, marks{a},1,moffs{a}, [], X, Y);
		end
		if (length(printd) > 0) ; print ('-dpdf', [printd anims{a} '_target.pdf']); end
	end
end

